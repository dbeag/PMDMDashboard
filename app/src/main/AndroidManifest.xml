<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.diegob.a6_dashboard">
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.6DashBoard">
        <activity
            android:name=".activities.DetalleActivityDisco"
            android:exported="false" />
        <activity
            android:name=".activities.MusicActivity"
            android:exported="true" />
        <activity
            android:name=".Splash"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activities.Splash_Quizzes"
            android:exported="true" />
        <activity
            android:name=".activities.Quizzes_Result_Activity"
            android:exported="true" />
        <activity
            android:name=".activities.Settings_activity"
            android:exported="true"
            android:theme="@style/Theme.Settings" />
        <activity
            android:name=".activities.Restaurantes_activity"
            android:exported="true"
            android:theme="@style/Theme.Restaurantes" />
        <activity
            android:name=".activities.Mapas_activity"
            android:exported="true"
            android:theme="@style/Theme.Mapas" />
        <activity
            android:name=".activities.Galeria_activity"
            android:exported="true"
            android:theme="@style/Theme.Galeria" />
        <activity
            android:name=".activities.Quizzes_activity"
            android:exported="true"
            android:theme="@style/Theme.Quizzes" />
        <activity
            android:name=".activities.Edad_activity"
            android:exported="true"
            android:theme="@style/Theme.Edad" />
        <activity
            android:name=".activities.Amigos_activity"
            android:exported="true"
            android:theme="@style/Theme.Amigos" />
        <activity
            android:name=".activities.Calculadora_activity"
            android:exported="true"
            android:theme="@style/Theme.Calculadora" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar" />
    </application>

</manifest>